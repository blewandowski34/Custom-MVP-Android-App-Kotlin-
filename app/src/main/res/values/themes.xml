<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CapstoneNoErrors" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#C10505</item>
        <item name="colorPrimaryVariant">#0609A3</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#C10505</item>
        <item name="colorSecondaryVariant">#0609A3</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#0609A3</item>
    </style>

    <style name="SeekBarTheme"
        parent="Widget.AppCompat.SeekBar">
        <item name="colorAccent">#C10505</item>
    </style>
</resources>